{
    "name": "ralfmaxxx/guzzle-http-message-factory",
    "description": "Implements php-http/message-factory interfaces for guzzle/psr7",
    "minimum-stability": "stable",
    "license": "BSD",
    "authors": [
        {
            "name": "Rafał Kański",
            "email": "rafal.kanski@schibsted.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Http\\Message\\Guzzle\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": ">=5.5",
        "guzzlehttp/psr7": "^1.3",
        "php-http/message-factory": "^1.0"
    },
    "provide": {
        "php-http/message-factory-implementation": "1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5",
        "phing/phing": "^2.14",
        "phpmd/phpmd": "^2.4",
        "squizlabs/php_codesniffer": "^2.6"
    }
}
